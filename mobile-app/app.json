{
  "expo": {
    "name": "Quick Order",
    "slug": "quick-order",
    "privacy": "public",
    "sdkVersion": "36.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/logo.png",
    "scheme": "myapp",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "android": {
      "package": "quick.order",
      "versionCode": 1,
      "googleServicesFile": "./google-services.json"
    },
    "ios": {
      "supportsTablet": true
    }
  }
}
